/**index.wxss**/
.logo-wrap {
  padding: 30rpx 25rpx;
}
.logo-wrap .content {
  box-sizing: border-box;
  width: 100%;
  height: 150rpx;
  padding: 0 20rpx;
  border-bottom: 1px dashed #999999;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.logo-wrap .name {
  font-size: 38rpx;
  /* font-weight: bold; */
  color: #333;
  margin-bottom: 10rpx;
}
.logo-wrap .slogan {
  font-size: 28rpx;
  color: #999;
}
.logo-wrap .logo {
  width: 100rpx;
  height: 100rpx;
}