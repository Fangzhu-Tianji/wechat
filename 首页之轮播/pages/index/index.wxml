<!--index.wxml-->
<view class="container">
  <swiper indicator-dots="true" interval="5000" duration="1000" circular="true">
    <block wx:for="{{imgUrls}}" wx:key="*this">
      <swiper-item>
        <image src="{{item}}" class="slide-image" width="355" height="150"/>
      </swiper-item>
    </block>
  </swiper>
</view>
