page {
  width: 100%;
  height: 100%;
}
.container {
  box-sizing: border-box;
  width: 100%;
  padding: 0 4% 2%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.wrap {
  width: 49%;
  height: 300rpx;
  margin-bottom: 15rpx;
}
.loading {
  display: block;
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
}
.goTop {
  width: 120rpx;
  height: 180rpx;
  position: fixed;
  right: 40rpx;
  bottom: 100rpx;
  z-index: 99;
}
.goTop image {
  display: block;
  width: 46rpx;
  height: 148rpx;
  margin: 0 auto;
}